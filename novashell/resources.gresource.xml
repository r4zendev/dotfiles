    <gresource prefix="/io/github/razen/novashell/styles">
        <file alias="main">styles/main.scss</file>
        <file alias="mixins">styles/_mixins.scss</file>
        <file alias="colors">styles/_colors.scss</file>
        <file alias="apps-window">styles/_apps-window.scss</file>
